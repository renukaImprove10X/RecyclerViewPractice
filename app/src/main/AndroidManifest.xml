<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.improve10x.recyclerviewpractice">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RecyclerViewPractice"
        tools:targetApi="31">
        <activity
            android:name=".AddEditSeriesActivity"
            android:exported="false" />
        <activity
            android:name=".whatsapp.messages.AddEditMessageActivity"
            android:exported="false" />
        <activity
            android:name=".whatsapp.templates.AddEditTemplateActivity"
            android:exported="false" />
        <activity
            android:name=".grocery.CategoriesActivity"
            android:exported="false" />
        <activity
            android:name=".filmDiary.series.SeriesActivity"
            android:exported="false" />
        <activity
            android:name=".filmDiary.movies.MoviesActivity"
            android:exported="false" />
        <activity
            android:name=".whatsapp.messages.MessageActivity"
            android:exported="false" />
        <activity
            android:name=".whatsapp.templates.TemplatesActivity"
            android:exported="true" />
        <activity
            android:name=".DashboardActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>